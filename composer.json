{
    "name": "wirke/select-blade-icons",
    "description": "An attempt at making a wrapper for Sage nad ACF composer to use Blade Icons more effectivly",
    "keywords": ["Blade", "Bootstrap Icons", "Laravel"],
    "homepage": "https://github.com/wwwirke/select-blade-icons",
    "license": "MIT",
    "authors": [{
        "name": "Filip Wessman",
        "email": "filip@wirke.se",
        "homepage": "https://wirke.se/",
        "role": "Developer"
    }],
    "require": {
        "php": "^7.4|^8.0",
        "blade-ui-kit/blade-icons": "^1.0",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Wirke\\SelectBladeIcons\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wirke\\SelectBladeIcons\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wirke\\SelectBladeIcons\\SelectBladeIconsProviderServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
